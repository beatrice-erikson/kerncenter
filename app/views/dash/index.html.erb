<style>
		.circle {
			display: inline-block;
			width: 100px; height: 100px;
			border: 15px solid;
	   		border-radius: 100px;
	   		text-decoration: none;
	   		color: black;
	   		background-color: white;
	   		transition: 0.3s;
	   		margin: 5px;
	   		box-shadow: -2px 2px 1px rgba(0,0,0,0.5);
	    }
	    .circle span {
	    	display: block;
	    	text-align: center;
			margin: 40px 5px;
			transition: inherit;
	    }
	    .circle:hover { width: 110px; height: 110px; margin: 0; }
		.circle:hover span { margin: 45px 5px; }
